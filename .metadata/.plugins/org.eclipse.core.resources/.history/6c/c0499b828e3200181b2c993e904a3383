
public abstract class Shape {
	
		private int length;
		private int width;
		
		public void setLength(int lgth) {
			length = lgth;
			}
		
		public int getLength() {
			return length;
		}
			
		public void setWidth(int wdth) {
			width = wdth;
		}
		
		public int getWidth() {
			return width;
		}


}
